<%= simple_form_for(@workstation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
      <%= f.input :serial %>
      <%= f.input :name %>
      <%= f.input :model %>
      <%= f.input :OS %>
      
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
